{
  "name": "Tournament",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "town": {
      "type": "string",
      "required": true
    },
    "format": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "isDateConfirmed": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "organizer": {
      "type": "string",
      "required": true
    },
    "information": {
      "type": "string"
    },
    "phoneContact": {
      "type": "string"
    },
    "emailContact": {
      "type": "string"
    },
    "adress": {
      "type": "string"
    },
    "googleId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "headJudge": {
      "type": "belongsTo",
      "model": "MyUser"
    },
    "availabilities": {
      "type": "hasMany",
      "model": "Availability",
      "foreignKey": "tournamentId"
    },
    "region": {
      "type": "belongsTo",
      "model": "Region",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
