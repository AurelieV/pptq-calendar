{
  "name": "Tournament",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "town": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "isDateConfirmed": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "organizer": {
      "type": "string",
      "required": true
    },
    "information": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "creator": {
      "type": "belongsTo",
      "model": "MyUser"
    },
    "availabilities": {
      "type": "hasMany",
      "model": "Availability"
    }
  },
  "acls": [],
  "methods": []
}
