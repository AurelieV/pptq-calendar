div(layout="row" layout-align="start center")
  md-input-container(ng-click="openPicker($event)")
    label {{ label }}
    input(type="text" ng-model="selected.model" disabled)
  md-button.md-raised.md-primary(ng-click="openPicker($event)")
    md-icon.material-icons.md-icon-button(md-font-icon="icon-event-note" aria-label="openCalendar")
  div(flex)
